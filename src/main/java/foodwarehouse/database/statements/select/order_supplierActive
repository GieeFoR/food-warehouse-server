SELECT `ORDER`.ORDER_ID AS 'ORDER.ORDER_ID',
       `ORDER`.PAYMENT_ID AS 'ORDER.PAYMENT_ID',
       `ORDER`.CUSTOMER_ID AS 'ORDER.CUSTOMER_ID',
       `ORDER`.DELIVERY_ID AS 'ORDER.DELIVERY_ID',
       `ORDER`.COMMENT AS 'ORDER.COMMENT',
       `ORDER`.ORDER_STATE AS 'ORDER.ORDER_STATE',
       `ORDER`.ORDER_DATE AS 'ORDER.ORDER_DATE',
       ORDER_PAYMENT.PAYMENT_ID AS 'ORDER_PAYMENT.PAYMENT_ID',
       ORDER_PAYMENT.PAYMENT_TYPE_ID AS 'ORDER_PAYMENT.PAYMENT_TYPE_ID',
       ORDER_PAYMENT.PAYMENT_VALUE AS 'ORDER_PAYMENT.PAYMENT_VALUE',
       ORDER_PAYMENT.PAYMENT_STATE AS 'ORDER_PAYMENT.PAYMENT_STATE',
       ORDER_PAYMENT_PAYMENT_TYPE.PAYMENT_TYPE_ID AS 'ORDER_PAYMENT_PAYMENT_TYPE.PAYMENT_TYPE_ID',
       ORDER_PAYMENT_PAYMENT_TYPE.PAYMENT_TYPE AS 'ORDER_PAYMENT_PAYMENT_TYPE.PAYMENT_TYPE',
       ORDER_CUSTOMER.CUSTOMER_ID AS 'ORDER_CUSTOMER.CUSTOMER_ID',
       ORDER_CUSTOMER.USER_ID AS 'ORDER_CUSTOMER.USER_ID',
       ORDER_CUSTOMER.ADDRESS_ID AS 'ORDER_CUSTOMER.ADDRESS_ID',
       ORDER_CUSTOMER.NAME AS 'ORDER_CUSTOMER.NAME',
       ORDER_CUSTOMER.SURNAME AS 'ORDER_CUSTOMER.SURNAME',
       ORDER_CUSTOMER.FIRM_NAME AS 'ORDER_CUSTOMER.FIRM_NAME',
       ORDER_CUSTOMER.TELEPHONE_NO AS 'ORDER_CUSTOMER.TELEPHONE_NO',
       ORDER_CUSTOMER.TAX_ID AS 'ORDER_CUSTOMER.TAX_ID',
       ORDER_CUSTOMER.DISCOUNT AS 'ORDER_CUSTOMER.DISCOUNT',
       ORDER_CUSTOMER_USER.USER_ID AS 'ORDER_CUSTOMER_USER.USER_ID',
       ORDER_CUSTOMER_USER.USERNAME AS 'ORDER_CUSTOMER_USER.USERNAME',
       ORDER_CUSTOMER_USER.PASSWORD AS 'ORDER_CUSTOMER_USER.PASSWORD',
       ORDER_CUSTOMER_USER.PERMISSION AS 'ORDER_CUSTOMER_USER.PERMISSION',
       ORDER_CUSTOMER_USER.E_MAIL AS 'ORDER_CUSTOMER_USER.E_MAIL',
       ORDER_CUSTOMER_ADDRESS.ADDRESS_ID AS 'ORDER_CUSTOMER_ADDRESS.ADDRESS_ID',
       ORDER_CUSTOMER_ADDRESS.COUNTRY AS 'ORDER_CUSTOMER_ADDRESS.COUNTRY',
       ORDER_CUSTOMER_ADDRESS.TOWN AS 'ORDER_CUSTOMER_ADDRESS.TOWN',
       ORDER_CUSTOMER_ADDRESS.POSTAL_CODE AS 'ORDER_CUSTOMER_ADDRESS.POSTAL_CODE',
       ORDER_CUSTOMER_ADDRESS.BUILDING_NO AS 'ORDER_CUSTOMER_ADDRESS.BUILDING_NO',
       ORDER_CUSTOMER_ADDRESS.STREET AS 'ORDER_CUSTOMER_ADDRESS.STREET',
       ORDER_CUSTOMER_ADDRESS.APARTMENT_NO AS 'ORDER_CUSTOMER_ADDRESS.APARTMENT_NO',
       ORDER_DELIVERY.DELIVERY_ID AS 'ORDER_DELIVERY.DELIVERY_ID',
       ORDER_DELIVERY.ADDRESS_ID AS 'ORDER_DELIVERY.ADDRESS_ID',
       ORDER_DELIVERY.SUPPLIER_ID AS 'ORDER_DELIVERY.SUPPLIER_ID',
       ORDER_DELIVERY.REMOVAL_FROM_STORAGE_DATE AS 'ORDER_DELIVERY.REMOVAL_FROM_STORAGE_DATE',
       ORDER_DELIVERY.DELIVERY_DATE AS 'ORDER_DELIVERY.DELIVERY_DATE',
       ORDER_DELIVERY_EMPLOYEE.EMPLOYEE_ID AS 'ORDER_DELIVERY_EMPLOYEE.EMPLOYEE_ID',
       ORDER_DELIVERY_EMPLOYEE.USER_ID AS 'ORDER_DELIVERY_EMPLOYEE.USER_ID',
       ORDER_DELIVERY_EMPLOYEE.NAME AS 'ORDER_DELIVERY_EMPLOYEE.NAME',
       ORDER_DELIVERY_EMPLOYEE.SURNAME AS 'ORDER_DELIVERY_EMPLOYEE.SURNAME',
       ORDER_DELIVERY_EMPLOYEE.POSITION AS 'ORDER_DELIVERY_EMPLOYEE.POSITION',
       ORDER_DELIVERY_EMPLOYEE.SALARY AS 'ORDER_DELIVERY_EMPLOYEE.SALARY',
       ORDER_DELIVERY_EMPLOYEE_USER.USER_ID AS 'ORDER_DELIVERY_EMPLOYEE_USER.USER_ID',
       ORDER_DELIVERY_EMPLOYEE_USER.USERNAME AS 'ORDER_DELIVERY_EMPLOYEE_USER.USERNAME',
       ORDER_DELIVERY_EMPLOYEE_USER.PASSWORD AS 'ORDER_DELIVERY_EMPLOYEE_USER.PASSWORD',
       ORDER_DELIVERY_EMPLOYEE_USER.PERMISSION AS 'ORDER_DELIVERY_EMPLOYEE_USER.PERMISSION',
       ORDER_DELIVERY_EMPLOYEE_USER.E_MAIL AS 'ORDER_DELIVERY_EMPLOYEE_USER.E_MAIL',
       ORDER_DELIVERY_ADDRESS.ADDRESS_ID AS 'ORDER_DELIVERY_ADDRESS.ADDRESS_ID',
       ORDER_DELIVERY_ADDRESS.COUNTRY AS 'ORDER_DELIVERY_ADDRESS.COUNTRY',
       ORDER_DELIVERY_ADDRESS.TOWN AS 'ORDER_DELIVERY_ADDRESS.TOWN',
       ORDER_DELIVERY_ADDRESS.POSTAL_CODE AS 'ORDER_DELIVERY_ADDRESS.POSTAL_CODE',
       ORDER_DELIVERY_ADDRESS.BUILDING_NO AS 'ORDER_DELIVERY_ADDRESS.BUILDING_NO',
       ORDER_DELIVERY_ADDRESS.STREET AS 'ORDER_DELIVERY_ADDRESS.STREET',
       ORDER_DELIVERY_ADDRESS.APARTMENT_NO AS 'ORDER_DELIVERY_ADDRESS.APARTMENT_NO'
FROM `ORDER`
JOIN `PAYMENT` `ORDER_PAYMENT` ON `ORDER`.`PAYMENT_ID` = `ORDER_PAYMENT`.`PAYMENT_ID`
JOIN `PAYMENT_TYPE` `ORDER_PAYMENT_PAYMENT_TYPE` ON `ORDER_PAYMENT`.`PAYMENT_TYPE_ID` = `ORDER_PAYMENT_PAYMENT_TYPE`.`PAYMENT_TYPE_ID`
JOIN `CUSTOMER` `ORDER_CUSTOMER` ON `ORDER`.`CUSTOMER_ID` = `ORDER_CUSTOMER`.`CUSTOMER_ID`
JOIN `USER` `ORDER_CUSTOMER_USER` ON `ORDER_CUSTOMER`.`USER_ID` = `ORDER_CUSTOMER_USER`.`USER_ID`
JOIN `ADDRESS` `ORDER_CUSTOMER_ADDRESS` ON `ORDER_CUSTOMER`.`ADDRESS_ID` = `ORDER_CUSTOMER_ADDRESS`.`ADDRESS_ID`
JOIN `DELIVERY` `ORDER_DELIVERY` ON `ORDER`.`DELIVERY_ID` = `ORDER_DELIVERY`.`DELIVERY_ID`
JOIN `EMPLOYEE` `ORDER_DELIVERY_EMPLOYEE` ON `ORDER_DELIVERY`.`SUPPLIER_ID` = `ORDER_DELIVERY_EMPLOYEE`.`EMPLOYEE_ID`
JOIN `USER` `ORDER_DELIVERY_EMPLOYEE_USER` ON `ORDER_DELIVERY_EMPLOYEE`.`USER_ID` = `ORDER_DELIVERY_EMPLOYEE_USER`.`USER_ID`
JOIN `ADDRESS` `ORDER_DELIVERY_ADDRESS` ON `ORDER_DELIVERY`.`ADDRESS_ID` = `ORDER_DELIVERY_ADDRESS`.`ADDRESS_ID`
WHERE `ORDER_DELIVERY_EMPLOYEE`.`EMPLOYEE_ID` = ?
AND `ORDER`.`ORDER_STATE` = "REGISTERED"
OR `ORDER`.`ORDER_STATE` = "COMPLETING"
OR `ORDER`.`ORDER_STATE` = "READY TO DELIVER"
OR `ORDER`.`ORDER_STATE` = "OUT FOR DELIVERY"
ORDER BY `ORDER`.`ORDER_ID` ASC;