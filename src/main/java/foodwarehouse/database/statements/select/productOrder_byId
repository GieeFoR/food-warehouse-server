       PRODUCT_ORDER_PRODUCT_BATCH_PRODUCT.MAKER_ID AS 'PRODUCT_ORDER_PRODUCT_BATCH_PRODUCT.MAKER_ID',
       PRODUCT_ORDER_PRODUCT_BATCH_PRODUCT.PRODUCT_NAME AS 'PRODUCT_ORDER_PRODUCT_BATCH_PRODUCT.PRODUCT_NAME',
       PRODUCT_ORDER_PRODUCT_BATCH_PRODUCT.SHORT_DESCRIPTION AS 'PRODUCT_ORDER_PRODUCT_BATCH_PRODUCT.SHORT_DESCRIPTION',
       PRODUCT_ORDER_PRODUCT_BATCH_PRODUCT.LONG_DESCRIPTION AS 'PRODUCT_ORDER_PRODUCT_BATCH_PRODUCT.LONG_DESCRIPTION',
       PRODUCT_ORDER_PRODUCT_BATCH_PRODUCT.CATEGORY AS 'PRODUCT_ORDER_PRODUCT_BATCH_PRODUCT.CATEGORY',
       PRODUCT_ORDER_PRODUCT_BATCH_PRODUCT.NEED_COLD_STORAGE AS 'PRODUCT_ORDER_PRODUCT_BATCH_PRODUCT.NEED_COLD_STORAGE',
       PRODUCT_ORDER_PRODUCT_BATCH_PRODUCT.BUY_PRICE AS 'PRODUCT_ORDER_PRODUCT_BATCH_PRODUCT.BUY_PRICE',
       PRODUCT_ORDER_PRODUCT_BATCH_PRODUCT.SELL_PRICE AS 'PRODUCT_ORDER_PRODUCT_BATCH_PRODUCT.SELL_PRICE',
       PRODUCT_ORDER_PRODUCT_BATCH_PRODUCT.IMAGE AS 'PRODUCT_ORDER_PRODUCT_BATCH_PRODUCT.IMAGE',
       PRODUCT_ORDER_PRODUCT_BATCH_PRODUCT_MAKER.MAKER_ID AS 'PRODUCT_ORDER_PRODUCT_BATCH_PRODUCT_MAKER.MAKER_ID',
       PRODUCT_ORDER_PRODUCT_BATCH_PRODUCT_MAKER.ADDRESS_ID AS 'PRODUCT_ORDER_PRODUCT_BATCH_PRODUCT_MAKER.ADDRESS_ID',
       PRODUCT_ORDER_PRODUCT_BATCH_PRODUCT_MAKER.FIRM_NAME AS 'PRODUCT_ORDER_PRODUCT_BATCH_PRODUCT_MAKER.FIRM_NAME',
       PRODUCT_ORDER_PRODUCT_BATCH_PRODUCT_MAKER.TELEPHONE_NO AS 'PRODUCT_ORDER_PRODUCT_BATCH_PRODUCT_MAKER.TELEPHONE_NO',
       PRODUCT_ORDER_PRODUCT_BATCH_PRODUCT_MAKER.E_MAIL AS 'PRODUCT_ORDER_PRODUCT_BATCH_PRODUCT_MAKER.E_MAIL',
       PRODUCT_ORDER_PRODUCT_BATCH_PRODUCT_MAKER_ADDRESS.ADDRESS_ID AS 'PRODUCT_ORDER_PRODUCT_BATCH_PRODUCT_MAKER_ADDRESS.ADDRESS_ID',
       PRODUCT_ORDER_PRODUCT_BATCH_PRODUCT_MAKER_ADDRESS.COUNTRY AS 'PRODUCT_ORDER_PRODUCT_BATCH_PRODUCT_MAKER_ADDRESS.COUNTRY',
       PRODUCT_ORDER_PRODUCT_BATCH_PRODUCT_MAKER_ADDRESS.TOWN AS 'PRODUCT_ORDER_PRODUCT_BATCH_PRODUCT_MAKER_ADDRESS.TOWN',
       PRODUCT_ORDER_PRODUCT_BATCH_PRODUCT_MAKER_ADDRESS.POSTAL_CODE AS 'PRODUCT_ORDER_PRODUCT_BATCH_PRODUCT_MAKER_ADDRESS.POSTAL_CODE',
       PRODUCT_ORDER_PRODUCT_BATCH_PRODUCT_MAKER_ADDRESS.BUILDING_NO AS 'PRODUCT_ORDER_PRODUCT_BATCH_PRODUCT_MAKER_ADDRESS.BUILDING_NO',
       PRODUCT_ORDER_PRODUCT_BATCH_PRODUCT_MAKER_ADDRESS.STREET AS 'PRODUCT_ORDER_PRODUCT_BATCH_PRODUCT_MAKER_ADDRESS.STREET',
       PRODUCT_ORDER_PRODUCT_BATCH_PRODUCT_MAKER_ADDRESS.APARTMENT_NO AS 'PRODUCT_ORDER_PRODUCT_BATCH_PRODUCT_MAKER_ADDRESS.APARTMENT_NO'
FROM `PRODUCT_ORDER`
JOIN `ORDER` `PRODUCT_ORDER_ORDER` ON `PRODUCT_ORDER`.`ORDER_ID` = `PRODUCT_ORDER_ORDER`.`ORDER_ID`
JOIN `PAYMENT` `PRODUCT_ORDER_ORDER_PAYMENT` ON `PRODUCT_ORDER_ORDER`.`PAYMENT_ID` = `PRODUCT_ORDER_ORDER_PAYMENT`.`PAYMENT_ID`
JOIN `PAYMENT_TYPE` `PRODUCT_ORDER_ORDER_PAYMENT_PAYMENT_TYPE` ON `PRODUCT_ORDER_ORDER_PAYMENT`.`PAYMENT_TYPE_ID` = `PRODUCT_ORDER_ORDER_PAYMENT_PAYMENT_TYPE`.`PAYMENT_TYPE_ID`
JOIN `CUSTOMER` `PRODUCT_ORDER_ORDER_CUSTOMER` ON `PRODUCT_ORDER_ORDER`.`CUSTOMER_ID` = `PRODUCT_ORDER_ORDER_CUSTOMER`.`CUSTOMER_ID`
JOIN `USER` `PRODUCT_ORDER_ORDER_CUSTOMER_USER` ON `PRODUCT_ORDER_ORDER_CUSTOMER`.`USER_ID` = `PRODUCT_ORDER_ORDER_CUSTOMER_USER`.`USER_ID`
JOIN `ADDRESS` `PRODUCT_ORDER_ORDER_CUSTOMER_ADDRESS` ON `PRODUCT_ORDER_ORDER_CUSTOMER`.`ADDRESS_ID` = `PRODUCT_ORDER_ORDER_CUSTOMER_ADDRESS`.`ADDRESS_ID`
JOIN `DELIVERY` `PRODUCT_ORDER_ORDER_DELIVERY` ON `PRODUCT_ORDER_ORDER`.`DELIVERY_ID` = `PRODUCT_ORDER_ORDER_DELIVERY`.`DELIVERY_ID`
JOIN `EMPLOYEE` `PRODUCT_ORDER_ORDER_DELIVERY_EMPLOYEE` ON `PRODUCT_ORDER_ORDER_DELIVERY`.`SUPPLIER_ID` = `PRODUCT_ORDER_ORDER_DELIVERY_EMPLOYEE`.`EMPLOYEE_ID`
JOIN `USER` `PRODUCT_ORDER_ORDER_DELIVERY_EMPLOYEE_USER` ON `PRODUCT_ORDER_ORDER_DELIVERY_EMPLOYEE`.`USER_ID` = `PRODUCT_ORDER_ORDER_DELIVERY_EMPLOYEE_USER`.`USER_ID`
JOIN `ADDRESS` `PRODUCT_ORDER_ORDER_DELIVERY_ADDRESS` ON `PRODUCT_ORDER_ORDER_DELIVERY`.`ADDRESS_ID` = `PRODUCT_ORDER_ORDER_DELIVERY_ADDRESS`.`ADDRESS_ID`
JOIN `PRODUCT_BATCH` `PRODUCT_ORDER_PRODUCT_BATCH` ON `PRODUCT_ORDER`.`BATCH_ID` = `PRODUCT_ORDER_PRODUCT_BATCH`.`BATCH_ID`
JOIN `PRODUCT` `PRODUCT_ORDER_PRODUCT_BATCH_PRODUCT` ON `PRODUCT_ORDER_PRODUCT_BATCH`.`PRODUCT_ID` = `PRODUCT_ORDER_PRODUCT_BATCH_PRODUCT`.`PRODUCT_ID`
JOIN `MAKER` `PRODUCT_ORDER_PRODUCT_BATCH_PRODUCT_MAKER` ON `PRODUCT_ORDER_PRODUCT_BATCH_PRODUCT`.`MAKER_ID` = `PRODUCT_ORDER_PRODUCT_BATCH_PRODUCT_MAKER`.`MAKER_ID`
JOIN `ADDRESS` `PRODUCT_ORDER_PRODUCT_BATCH_PRODUCT_MAKER_ADDRESS` ON `PRODUCT_ORDER_PRODUCT_BATCH_PRODUCT_MAKER`.`ADDRESS_ID` = `PRODUCT_ORDER_PRODUCT_BATCH_PRODUCT_MAKER_ADDRESS`.`ADDRESS_ID`
WHERE `PRODUCT_ORDER`.`ORDER_ID` = ?
AND `PRODUCT_ORDER`.`BATCH_ID` = ?;