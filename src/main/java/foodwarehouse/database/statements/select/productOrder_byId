       PRODUCT_ORDER_ORDER_PAYMENT.PAYMENT_VALUE AS 'PRODUCT_ORDER_ORDER_PAYMENT.PAYMENT_VALUE',
       PRODUCT_ORDER_ORDER_PAYMENT.PAYMENT_STATE AS 'PRODUCT_ORDER_ORDER_PAYMENT.PAYMENT_STATE',
       PRODUCT_ORDER_ORDER_PAYMENT_PAYMENT_TYPE.PAYMENT_TYPE_ID AS 'PRODUCT_ORDER_ORDER_PAYMENT_PAYMENT_TYPE.PAYMENT_TYPE_ID',
       PRODUCT_ORDER_ORDER_PAYMENT_PAYMENT_TYPE.PAYMENT_TYPE AS 'PRODUCT_ORDER_ORDER_PAYMENT_PAYMENT_TYPE.PAYMENT_TYPE',
       PRODUCT_ORDER_ORDER_CUSTOMER.CUSTOMER_ID AS 'PRODUCT_ORDER_ORDER_CUSTOMER.CUSTOMER_ID',
       PRODUCT_ORDER_ORDER_CUSTOMER.USER_ID AS 'PRODUCT_ORDER_ORDER_CUSTOMER.USER_ID',
       PRODUCT_ORDER_ORDER_CUSTOMER.ADDRESS_ID AS 'PRODUCT_ORDER_ORDER_CUSTOMER.ADDRESS_ID',
       PRODUCT_ORDER_ORDER_CUSTOMER.NAME AS 'PRODUCT_ORDER_ORDER_CUSTOMER.NAME',
       PRODUCT_ORDER_ORDER_CUSTOMER.SURNAME AS 'PRODUCT_ORDER_ORDER_CUSTOMER.SURNAME',
       PRODUCT_ORDER_ORDER_CUSTOMER.FIRM_NAME AS 'PRODUCT_ORDER_ORDER_CUSTOMER.FIRM_NAME',
       PRODUCT_ORDER_ORDER_CUSTOMER.TELEPHONE_NO AS 'PRODUCT_ORDER_ORDER_CUSTOMER.TELEPHONE_NO',
       PRODUCT_ORDER_ORDER_CUSTOMER.TAX_ID AS 'PRODUCT_ORDER_ORDER_CUSTOMER.TAX_ID',
       PRODUCT_ORDER_ORDER_CUSTOMER.DISCOUNT AS 'PRODUCT_ORDER_ORDER_CUSTOMER.DISCOUNT',
       PRODUCT_ORDER_ORDER_CUSTOMER_USER.USER_ID AS 'PRODUCT_ORDER_ORDER_CUSTOMER_USER.USER_ID',
       PRODUCT_ORDER_ORDER_CUSTOMER_USER.USERNAME AS 'PRODUCT_ORDER_ORDER_CUSTOMER_USER.USERNAME',
       PRODUCT_ORDER_ORDER_CUSTOMER_USER.PASSWORD AS 'PRODUCT_ORDER_ORDER_CUSTOMER_USER.PASSWORD',
       PRODUCT_ORDER_ORDER_CUSTOMER_USER.PERMISSION AS 'PRODUCT_ORDER_ORDER_CUSTOMER_USER.PERMISSION',
       PRODUCT_ORDER_ORDER_CUSTOMER_USER.E_MAIL AS 'PRODUCT_ORDER_ORDER_CUSTOMER_USER.E_MAIL',
       PRODUCT_ORDER_ORDER_CUSTOMER_ADDRESS.ADDRESS_ID AS 'PRODUCT_ORDER_ORDER_CUSTOMER_ADDRESS.ADDRESS_ID',
       PRODUCT_ORDER_ORDER_CUSTOMER_ADDRESS.COUNTRY AS 'PRODUCT_ORDER_ORDER_CUSTOMER_ADDRESS.COUNTRY',
       PRODUCT_ORDER_ORDER_CUSTOMER_ADDRESS.TOWN AS 'PRODUCT_ORDER_ORDER_CUSTOMER_ADDRESS.TOWN',
       PRODUCT_ORDER_ORDER_CUSTOMER_ADDRESS.POSTAL_CODE AS 'PRODUCT_ORDER_ORDER_CUSTOMER_ADDRESS.POSTAL_CODE',
       PRODUCT_ORDER_ORDER_CUSTOMER_ADDRESS.BUILDING_NO AS 'PRODUCT_ORDER_ORDER_CUSTOMER_ADDRESS.BUILDING_NO',
       PRODUCT_ORDER_ORDER_CUSTOMER_ADDRESS.STREET AS 'PRODUCT_ORDER_ORDER_CUSTOMER_ADDRESS.STREET',
       PRODUCT_ORDER_ORDER_CUSTOMER_ADDRESS.APARTMENT_NO AS 'PRODUCT_ORDER_ORDER_CUSTOMER_ADDRESS.APARTMENT_NO',
       PRODUCT_ORDER_ORDER_DELIVERY.DELIVERY_ID AS 'PRODUCT_ORDER_ORDER_DELIVERY.DELIVERY_ID',
       PRODUCT_ORDER_ORDER_DELIVERY.ADDRESS_ID AS 'PRODUCT_ORDER_ORDER_DELIVERY.ADDRESS_ID',
       PRODUCT_ORDER_ORDER_DELIVERY.SUPPLIER_ID AS 'PRODUCT_ORDER_ORDER_DELIVERY.SUPPLIER_ID',
       PRODUCT_ORDER_ORDER_DELIVERY.REMOVAL_FROM_STORAGE_DATE AS 'PRODUCT_ORDER_ORDER_DELIVERY.REMOVAL_FROM_STORAGE_DATE',
       PRODUCT_ORDER_ORDER_DELIVERY.DELIVERY_DATE AS 'PRODUCT_ORDER_ORDER_DELIVERY.DELIVERY_DATE',
       PRODUCT_ORDER_ORDER_DELIVERY_EMPLOYEE.EMPLOYEE_ID AS 'PRODUCT_ORDER_ORDER_DELIVERY_EMPLOYEE.EMPLOYEE_ID',
