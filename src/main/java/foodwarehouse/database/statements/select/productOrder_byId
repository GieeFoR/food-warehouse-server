       PRODUCT_ORDER_ORDER_DELIVERY_ADDRESS.APARTMENT_NO AS 'PRODUCT_ORDER_ORDER_DELIVERY_ADDRESS.APARTMENT_NO',
       PRODUCT_ORDER_PRODUCT_BATCH.BATCH_ID AS 'PRODUCT_ORDER_PRODUCT_BATCH.BATCH_ID',
       PRODUCT_ORDER_PRODUCT_BATCH.PRODUCT_ID AS 'PRODUCT_ORDER_PRODUCT_BATCH.PRODUCT_ID',
       PRODUCT_ORDER_PRODUCT_BATCH.BATCH_NO AS 'PRODUCT_ORDER_PRODUCT_BATCH.BATCH_NO',
       PRODUCT_ORDER_PRODUCT_BATCH.EAT_BY_DATE AS 'PRODUCT_ORDER_PRODUCT_BATCH.EAT_BY_DATE',
       PRODUCT_ORDER_PRODUCT_BATCH.DISCOUNT AS 'PRODUCT_ORDER_PRODUCT_BATCH.DISCOUNT',
       PRODUCT_ORDER_PRODUCT_BATCH.PACKAGES_QUANTITY AS 'PRODUCT_ORDER_PRODUCT_BATCH.PACKAGES_QUANTITY',
