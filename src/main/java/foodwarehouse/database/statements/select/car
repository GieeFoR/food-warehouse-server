SELECT CAR.CAR_ID AS 'CAR.CAR_ID',
       CAR.DRIVER_ID AS 'CAR.DRIVER_ID',
       CAR.BRAND AS 'CAR.BRAND',
       CAR.MODEL AS 'CAR.MODEL',
       CAR.YEAR_OF_PROD AS 'CAR.YEAR_OF_PROD',
       CAR.REGISTRATION_NO AS 'CAR.REGISTRATION_NO',
       CAR.INSURANCE_EXP AS 'CAR.INSURANCE_EXP',
       CAR.INSPECTION_EXP AS 'CAR.INSPECTION_EXP',
       CAR_EMPLOYEE.EMPLOYEE_ID AS 'CAR_EMPLOYEE.EMPLOYEE_ID',
       CAR_EMPLOYEE.USER_ID AS 'CAR_EMPLOYEE.USER_ID',
       CAR_EMPLOYEE.NAME AS 'CAR_EMPLOYEE.NAME',
       CAR_EMPLOYEE.SURNAME AS 'CAR_EMPLOYEE.SURNAME',
       CAR_EMPLOYEE.POSITION AS 'CAR_EMPLOYEE.POSITION',
       CAR_EMPLOYEE.SALARY AS 'CAR_EMPLOYEE.SALARY',
       CAR_EMPLOYEE_USER.USER_ID AS 'CAR_EMPLOYEE_USER.USER_ID',
       CAR_EMPLOYEE_USER.USERNAME AS 'CAR_EMPLOYEE_USER.USERNAME',
       CAR_EMPLOYEE_USER.PASSWORD AS 'CAR_EMPLOYEE_USER.PASSWORD',
       CAR_EMPLOYEE_USER.PERMISSION AS 'CAR_EMPLOYEE_USER.PERMISSION',
       CAR_EMPLOYEE_USER.E_MAIL AS 'CAR_EMPLOYEE_USER.E_MAIL'
FROM `CAR`
         JOIN `EMPLOYEE` `CAR_EMPLOYEE` ON `CAR`.`DRIVER_ID` = `CAR_EMPLOYEE`.`EMPLOYEE_ID`
         JOIN `USER` `CAR_EMPLOYEE_USER` ON `CAR_EMPLOYEE`.`USER_ID` = `CAR_EMPLOYEE_USER`.`USER_ID`;