SELECT DELIVERY.DELIVERY_ID AS 'DELIVERY.DELIVERY_ID',
       DELIVERY.ADDRESS_ID AS 'DELIVERY.ADDRESS_ID',
       DELIVERY.SUPPLIER_ID AS 'DELIVERY.SUPPLIER_ID',
       DELIVERY.REMOVAL_FROM_STORAGE_DATE AS 'DELIVERY.REMOVAL_FROM_STORAGE_DATE',
       DELIVERY.DELIVERY_ID AS 'DELIVERY.DELIVERY_ID',
       DELIVERY_EMPLOYEE.EMPLOYEE_ID AS 'DELIVERY_EMPLOYEE.EMPLOYEE_ID',
       DELIVERY_EMPLOYEE.USER_ID AS 'DELIVERY_EMPLOYEE.USER_ID',
       DELIVERY_EMPLOYEE.NAME AS 'DELIVERY_EMPLOYEE.NAME',
       DELIVERY_EMPLOYEE.SURNAME AS 'DELIVERY_EMPLOYEE.SURNAME',
       DELIVERY_EMPLOYEE.POSITION AS 'DELIVERY_EMPLOYEE.POSITION',
       DELIVERY_EMPLOYEE.SALARY AS 'DELIVERY_EMPLOYEE.SALARY',
       DELIVERY_EMPLOYEE_USER.USER_ID AS 'DELIVERY_EMPLOYEE_USER.USER_ID',
       DELIVERY_EMPLOYEE_USER.USERNAME AS 'DELIVERY_EMPLOYEE_USER.USERNAME',
       DELIVERY_EMPLOYEE_USER.PASSWORD AS 'DELIVERY_EMPLOYEE_USER.PASSWORD',
       DELIVERY_EMPLOYEE_USER.PERMISSION AS 'DELIVERY_EMPLOYEE_USER.PERMISSION',
       DELIVERY_EMPLOYEE_USER.E_MAIL AS 'DELIVERY_EMPLOYEE_USER.E_MAIL',
       DELIVERY_ADDRESS.ADDRESS_ID AS 'DELIVERY_ADDRESS.ADDRESS_ID',
       DELIVERY_ADDRESS.COUNTRY AS 'DELIVERY_ADDRESS.COUNTRY',
       DELIVERY_ADDRESS.TOWN AS 'DELIVERY_ADDRESS.TOWN',
       DELIVERY_ADDRESS.POSTAL_CODE AS 'DELIVERY_ADDRESS.POSTAL_CODE',
       DELIVERY_ADDRESS.BUILDING_NO AS 'DELIVERY_ADDRESS.BUILDING_NO',
       DELIVERY_ADDRESS.STREET AS 'DELIVERY_ADDRESS.STREET',
       DELIVERY_ADDRESS.APARTMENT_NO AS 'DELIVERY_ADDRESS.APARTMENT_NO'
FROM `DELIVERY`
JOIN `ADDRESS` `DELIVERY_ADDRESS` ON `DELIVERY`.`ADDRESS_ID` = `DELIVERY_ADDRESS`.`ADDRESS_ID`
JOIN `EMPLOYEE` `DELIVERY_EMPLOYEE` ON `DELIVERY`.`SUPPLIER_ID` = `DELIVERY_EMPLOYEE`.`EMPLOYEE_ID`
JOIN `USER` `DELIVERY_EMPLOYEE_USER` ON `DELIVERY_EMPLOYEE`.`USER_ID` = `DELIVERY_EMPLOYEE_USER`.`USER_ID`
WHERE `DELIVERY_ID` = ?;