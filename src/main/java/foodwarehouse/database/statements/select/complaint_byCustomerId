SELECT COMPLAINT.COMPLAINT_ID AS 'COMPLAINT.COMPLAINT_ID',
       COMPLAINT.ORDER_ID AS 'COMPLAINT.ORDER_ID',
       COMPLAINT.CONTENT AS 'COMPLAINT.CONTENT',
       COMPLAINT.SEND_DATE AS 'COMPLAINT.SEND_DATE',
       COMPLAINT.COMPLAINT_STATE AS 'COMPLAINT.COMPLAINT_STATE',
       COMPLAINT.DECISION AS 'COMPLAINT.DECISION',
       COMPLAINT.DECISION_DATE AS 'COMPLAINT.DECISION_DATE',
       COMPLAINT_ORDER.ORDER_ID AS 'COMPLAINT_ORDER.ORDER_ID',
       COMPLAINT_ORDER.PAYMENT_ID AS 'COMPLAINT_ORDER.PAYMENT_ID',
       COMPLAINT_ORDER.CUSTOMER_ID AS 'COMPLAINT_ORDER.CUSTOMER_ID',
       COMPLAINT_ORDER.DELIVERY_ID AS 'COMPLAINT_ORDER.DELIVERY_ID',
       COMPLAINT_ORDER.COMMENT AS 'COMPLAINT_ORDER.COMMENT',
       COMPLAINT_ORDER.ORDER_STATE AS 'COMPLAINT_ORDER.ORDER_STATE',
       COMPLAINT_ORDER.ORDER_DATE AS 'COMPLAINT_ORDER.ORDER_DATE',
       COMPLAINT_ORDER_PAYMENT.PAYMENT_ID AS 'COMPLAINT_ORDER_PAYMENT.PAYMENT_ID',
       COMPLAINT_ORDER_PAYMENT.PAYMENT_TYPE_ID AS 'COMPLAINT_ORDER_PAYMENT.PAYMENT_TYPE_ID',
       COMPLAINT_ORDER_PAYMENT.PAYMENT_VALUE AS 'COMPLAINT_ORDER_PAYMENT.PAYMENT_VALUE',
       COMPLAINT_ORDER_PAYMENT.PAYMENT_STATE AS 'COMPLAINT_ORDER_PAYMENT.PAYMENT_STATE',
       COMPLAINT_ORDER_PAYMENT_PAYMENT_TYPE.PAYMENT_TYPE_ID AS 'COMPLAINT_ORDER_PAYMENT_PAYMENT_TYPE.PAYMENT_TYPE_ID',
       COMPLAINT_ORDER_PAYMENT_PAYMENT_TYPE.PAYMENT_TYPE AS 'COMPLAINT_ORDER_PAYMENT_PAYMENT_TYPE.PAYMENT_TYPE',
       COMPLAINT_ORDER_CUSTOMER.CUSTOMER_ID AS 'COMPLAINT_ORDER_CUSTOMER.CUSTOMER_ID',
       COMPLAINT_ORDER_CUSTOMER.USER_ID AS 'COMPLAINT_ORDER_CUSTOMER.USER_ID',
       COMPLAINT_ORDER_CUSTOMER.ADDRESS_ID AS 'COMPLAINT_ORDER_CUSTOMER.ADDRESS_ID',
       COMPLAINT_ORDER_CUSTOMER.NAME AS 'COMPLAINT_ORDER_CUSTOMER.NAME',
       COMPLAINT_ORDER_CUSTOMER.SURNAME AS 'COMPLAINT_ORDER_CUSTOMER.SURNAME',
       COMPLAINT_ORDER_CUSTOMER.FIRM_NAME AS 'COMPLAINT_ORDER_CUSTOMER.FIRM_NAME',
       COMPLAINT_ORDER_CUSTOMER.TELEPHONE_NO AS 'COMPLAINT_ORDER_CUSTOMER.TELEPHONE_NO',
       COMPLAINT_ORDER_CUSTOMER.TAX_ID AS 'COMPLAINT_ORDER_CUSTOMER.TAX_ID',
       COMPLAINT_ORDER_CUSTOMER.DISCOUNT AS 'COMPLAINT_ORDER_CUSTOMER.DISCOUNT',
       COMPLAINT_ORDER_CUSTOMER_USER.USER_ID AS 'COMPLAINT_ORDER_CUSTOMER_USER.USER_ID',
       COMPLAINT_ORDER_CUSTOMER_USER.USERNAME AS 'COMPLAINT_ORDER_CUSTOMER_USER.USERNAME',
       COMPLAINT_ORDER_CUSTOMER_USER.PASSWORD AS 'COMPLAINT_ORDER_CUSTOMER_USER.PASSWORD',
       COMPLAINT_ORDER_CUSTOMER_USER.PERMISSION AS 'COMPLAINT_ORDER_CUSTOMER_USER.PERMISSION',
       COMPLAINT_ORDER_CUSTOMER_USER.E_MAIL AS 'COMPLAINT_ORDER_CUSTOMER_USER.E_MAIL',
       COMPLAINT_ORDER_CUSTOMER_ADDRESS.ADDRESS_ID AS 'COMPLAINT_ORDER_CUSTOMER_ADDRESS.ADDRESS_ID',
       COMPLAINT_ORDER_CUSTOMER_ADDRESS.COUNTRY AS 'COMPLAINT_ORDER_CUSTOMER_ADDRESS.COUNTRY',
       COMPLAINT_ORDER_CUSTOMER_ADDRESS.TOWN AS 'COMPLAINT_ORDER_CUSTOMER_ADDRESS.TOWN',
       COMPLAINT_ORDER_CUSTOMER_ADDRESS.POSTAL_CODE AS 'COMPLAINT_ORDER_CUSTOMER_ADDRESS.POSTAL_CODE',
       COMPLAINT_ORDER_CUSTOMER_ADDRESS.BUILDING_NO AS 'COMPLAINT_ORDER_CUSTOMER_ADDRESS.BUILDING_NO',
       COMPLAINT_ORDER_CUSTOMER_ADDRESS.STREET AS 'COMPLAINT_ORDER_CUSTOMER_ADDRESS.STREET',
       COMPLAINT_ORDER_CUSTOMER_ADDRESS.APARTMENT_NO AS 'COMPLAINT_ORDER_CUSTOMER_ADDRESS.APARTMENT_NO',
       COMPLAINT_ORDER_DELIVERY.DELIVERY_ID AS 'COMPLAINT_ORDER_DELIVERY.DELIVERY_ID',
       COMPLAINT_ORDER_DELIVERY.ADDRESS_ID AS 'COMPLAINT_ORDER_DELIVERY.ADDRESS_ID',
       COMPLAINT_ORDER_DELIVERY.SUPPLIER_ID AS 'COMPLAINT_ORDER_DELIVERY.SUPPLIER_ID',
       COMPLAINT_ORDER_DELIVERY.REMOVAL_FROM_STORAGE_DATE AS 'COMPLAINT_ORDER_DELIVERY.REMOVAL_FROM_STORAGE_DATE',
       COMPLAINT_ORDER_DELIVERY.DELIVERY_DATE AS 'COMPLAINT_ORDER_DELIVERY.DELIVERY_DATE',
       COMPLAINT_ORDER_DELIVERY_EMPLOYEE.EMPLOYEE_ID AS 'COMPLAINT_ORDER_DELIVERY_EMPLOYEE.EMPLOYEE_ID',
       COMPLAINT_ORDER_DELIVERY_EMPLOYEE.USER_ID AS 'COMPLAINT_ORDER_DELIVERY_EMPLOYEE.USER_ID',
       COMPLAINT_ORDER_DELIVERY_EMPLOYEE.NAME AS 'COMPLAINT_ORDER_DELIVERY_EMPLOYEE.NAME',
       COMPLAINT_ORDER_DELIVERY_EMPLOYEE.SURNAME AS 'COMPLAINT_ORDER_DELIVERY_EMPLOYEE.SURNAME',
       COMPLAINT_ORDER_DELIVERY_EMPLOYEE.POSITION AS 'COMPLAINT_ORDER_DELIVERY_EMPLOYEE.POSITION',
       COMPLAINT_ORDER_DELIVERY_EMPLOYEE.SALARY AS 'COMPLAINT_ORDER_DELIVERY_EMPLOYEE.SALARY',
       COMPLAINT_ORDER_DELIVERY_EMPLOYEE_USER.USER_ID AS 'COMPLAINT_ORDER_DELIVERY_EMPLOYEE_USER.USER_ID',
       COMPLAINT_ORDER_DELIVERY_EMPLOYEE_USER.USERNAME AS 'COMPLAINT_ORDER_DELIVERY_EMPLOYEE_USER.USERNAME',
       COMPLAINT_ORDER_DELIVERY_EMPLOYEE_USER.PASSWORD AS 'COMPLAINT_ORDER_DELIVERY_EMPLOYEE_USER.PASSWORD',
       COMPLAINT_ORDER_DELIVERY_EMPLOYEE_USER.PERMISSION AS 'COMPLAINT_ORDER_DELIVERY_EMPLOYEE_USER.PERMISSION',
       COMPLAINT_ORDER_DELIVERY_EMPLOYEE_USER.E_MAIL AS 'COMPLAINT_ORDER_DELIVERY_EMPLOYEE_USER.E_MAIL',
       COMPLAINT_ORDER_DELIVERY_ADDRESS.ADDRESS_ID AS 'COMPLAINT_ORDER_DELIVERY_ADDRESS.ADDRESS_ID',
       COMPLAINT_ORDER_DELIVERY_ADDRESS.COUNTRY AS 'COMPLAINT_ORDER_DELIVERY_ADDRESS.COUNTRY',
       COMPLAINT_ORDER_DELIVERY_ADDRESS.TOWN AS 'COMPLAINT_ORDER_DELIVERY_ADDRESS.TOWN',
       COMPLAINT_ORDER_DELIVERY_ADDRESS.POSTAL_CODE AS 'COMPLAINT_ORDER_DELIVERY_ADDRESS.POSTAL_CODE',
       COMPLAINT_ORDER_DELIVERY_ADDRESS.BUILDING_NO AS 'COMPLAINT_ORDER_DELIVERY_ADDRESS.BUILDING_NO',
       COMPLAINT_ORDER_DELIVERY_ADDRESS.STREET AS 'COMPLAINT_ORDER_DELIVERY_ADDRESS.STREET',
       COMPLAINT_ORDER_DELIVERY_ADDRESS.APARTMENT_NO AS 'COMPLAINT_ORDER_DELIVERY_ADDRESS.APARTMENT_NO'
FROM `COMPLAINT`
JOIN `ORDER` `COMPLAINT_ORDER` ON `COMPLAINT`.`ORDER_ID` = `COMPLAINT_ORDER`.`ORDER_ID`
JOIN `PAYMENT` `COMPLAINT_ORDER_PAYMENT` ON `COMPLAINT_ORDER`.`PAYMENT_ID` = `COMPLAINT_ORDER_PAYMENT`.`PAYMENT_ID`
JOIN `PAYMENT_TYPE` `COMPLAINT_ORDER_PAYMENT_PAYMENT_TYPE` ON `COMPLAINT_ORDER_PAYMENT`.`PAYMENT_TYPE_ID` = `COMPLAINT_ORDER_PAYMENT_PAYMENT_TYPE`.`PAYMENT_TYPE_ID`
JOIN `CUSTOMER` `COMPLAINT_ORDER_CUSTOMER` ON `COMPLAINT_ORDER`.`CUSTOMER_ID` = `COMPLAINT_ORDER_CUSTOMER`.`CUSTOMER_ID`
JOIN `USER` `COMPLAINT_ORDER_CUSTOMER_USER` ON `COMPLAINT_ORDER_CUSTOMER`.`USER_ID` = `COMPLAINT_ORDER_CUSTOMER_USER`.`USER_ID`
JOIN `ADDRESS` `COMPLAINT_ORDER_CUSTOMER_ADDRESS` ON `COMPLAINT_ORDER_CUSTOMER`.`ADDRESS_ID` = `COMPLAINT_ORDER_CUSTOMER_ADDRESS`.`ADDRESS_ID`
JOIN `DELIVERY` `COMPLAINT_ORDER_DELIVERY` ON `COMPLAINT_ORDER`.`DELIVERY_ID` = `COMPLAINT_ORDER_DELIVERY`.`DELIVERY_ID`
JOIN `EMPLOYEE` `COMPLAINT_ORDER_DELIVERY_EMPLOYEE` ON `COMPLAINT_ORDER_DELIVERY`.`SUPPLIER_ID` = `COMPLAINT_ORDER_DELIVERY_EMPLOYEE`.`EMPLOYEE_ID`
JOIN `USER` `COMPLAINT_ORDER_DELIVERY_EMPLOYEE_USER` ON `COMPLAINT_ORDER_DELIVERY_EMPLOYEE`.`USER_ID` = `COMPLAINT_ORDER_DELIVERY_EMPLOYEE_USER`.`USER_ID`
JOIN `ADDRESS` `COMPLAINT_ORDER_DELIVERY_ADDRESS` ON `COMPLAINT_ORDER_DELIVERY`.`ADDRESS_ID` = `COMPLAINT_ORDER_DELIVERY_ADDRESS`.`ADDRESS_ID`
WHERE `COMPLAINT_ORDER_CUSTOMER`.`CUSTOMER_ID` = ?;