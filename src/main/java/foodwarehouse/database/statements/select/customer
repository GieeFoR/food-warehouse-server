SELECT CUSTOMER.CUSTOMER_ID AS 'CUSTOMER.CUSTOMER_ID',
       CUSTOMER.USER_ID AS 'CUSTOMER.USER_ID',
       CUSTOMER.ADDRESS_ID AS 'CUSTOMER.ADDRESS_ID',
       CUSTOMER.NAME AS 'CUSTOMER.NAME',
       CUSTOMER.SURNAME AS 'CUSTOMER.SURNAME',
       CUSTOMER.FIRM_NAME AS 'CUSTOMER.FIRM_NAME',
       CUSTOMER.TELEPHONE_NO AS 'CUSTOMER.TELEPHONE_NO',
       CUSTOMER.TAX_ID AS 'CUSTOMER.TAX_ID',
       CUSTOMER.DISCOUNT AS 'CUSTOMER.DISCOUNT',
       CUSTOMER_USER.USER_ID AS 'CUSTOMER_USER.USER_ID',
       CUSTOMER_USER.USERNAME AS 'CUSTOMER_USER.USERNAME',
       CUSTOMER_USER.PASSWORD AS 'CUSTOMER_USER.PASSWORD',
       CUSTOMER_USER.PERMISSION AS 'CUSTOMER_USER.PERMISSION',
       CUSTOMER_USER.E_MAIL AS 'CUSTOMER_USER.E_MAIL',
       CUSTOMER_ADDRESS.ADDRESS_ID AS 'CUSTOMER_ADDRESS.ADDRESS_ID',
       CUSTOMER_ADDRESS.COUNTRY AS 'CUSTOMER_ADDRESS.COUNTRY',
       CUSTOMER_ADDRESS.TOWN AS 'CUSTOMER_ADDRESS.TOWN',
       CUSTOMER_ADDRESS.POSTAL_CODE AS 'CUSTOMER_ADDRESS.POSTAL_CODE',
       CUSTOMER_ADDRESS.BUILDING_NO AS 'CUSTOMER_ADDRESS.BUILDING_NO',
       CUSTOMER_ADDRESS.STREET AS 'CUSTOMER_ADDRESS.STREET',
       CUSTOMER_ADDRESS.APARTMENT_NO AS 'CUSTOMER_ADDRESS.APARTMENT_NO'
FROM `CUSTOMER`
JOIN `USER` `CUSTOMER_USER` ON `CUSTOMER`.`USER_ID` = `CUSTOMER_USER`.`USER_ID`
JOIN `ADDRESS` `CUSTOMER_ADDRESS` ON `CUSTOMER`.`ADDRESS_ID` = `CUSTOMER_ADDRESS`.`ADDRESS_ID`;