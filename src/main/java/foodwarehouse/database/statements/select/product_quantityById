SELECT SUM(`PRODUCT_IN_STORAGE`.`QUANTITY`) `RESULT`
FROM `PRODUCT_IN_STORAGE`
JOIN `PRODUCT_BATCH` ON `PRODUCT_IN_STORAGE`.`BATCH_ID` = `PRODUCT_BATCH`.`BATCH_ID`
JOIN `PRODUCT` ON `PRODUCT`.`PRODUCT_ID` = `PRODUCT_BATCH`.`PRODUCT_ID`
WHERE `PRODUCT_BATCH`.`EAT_BY_DATE` > DATEADD('DAY',14, CURRENT_DATE)
AND `PRODUCT`.`PRODUCT_ID` = ?;