SELECT ADDRESS.ADDRESS_ID AS 'ADDRESS.ADDRESS_ID',
       ADDRESS.COUNTRY AS 'ADDRESS.COUNTRY',
       ADDRESS.TOWN AS 'ADDRESS.TOWN',
       ADDRESS.POSTAL_CODE AS 'ADDRESS.POSTAL_CODE',
       ADDRESS.BUILDING_NO AS 'ADDRESS.BUILDING_NO',
       ADDRESS.STREET AS 'ADDRESS.STREET',
       ADDRESS.APARTMENT_NO AS 'ADDRESS.APARTMENT_NO'
FROM `ADDRESS`
WHERE `ADDRESS_ID` = ?;