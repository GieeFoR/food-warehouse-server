SELECT PRODUCT_IN_STORAGE.BATCH_ID AS 'PRODUCT_IN_STORAGE.BATCH_ID',
       PRODUCT_IN_STORAGE.STORAGE_ID AS 'PRODUCT_IN_STORAGE.STORAGE_ID',
       PRODUCT_IN_STORAGE.QUANTITY AS 'PRODUCT_IN_STORAGE.QUANTITY',
       PRODUCT_IN_STORAGE_PRODUCT_BATCH.BATCH_ID AS 'PRODUCT_IN_STORAGE_PRODUCT_BATCH.BATCH_ID',
       PRODUCT_IN_STORAGE_PRODUCT_BATCH.PRODUCT_ID AS 'PRODUCT_IN_STORAGE_PRODUCT_BATCH.PRODUCT_ID',
       PRODUCT_IN_STORAGE_PRODUCT_BATCH.BATCH_NO AS 'PRODUCT_IN_STORAGE_PRODUCT_BATCH.BATCH_NO',
       PRODUCT_IN_STORAGE_PRODUCT_BATCH.EAT_BY_DATE AS 'PRODUCT_IN_STORAGE_PRODUCT_BATCH.EAT_BY_DATE',
       PRODUCT_IN_STORAGE_PRODUCT_BATCH.DISCOUNT AS 'PRODUCT_IN_STORAGE_PRODUCT_BATCH.DISCOUNT',
       PRODUCT_IN_STORAGE_PRODUCT_BATCH.PACKAGES_QUANTITY AS 'PRODUCT_IN_STORAGE_PRODUCT_BATCH.PACKAGES_QUANTITY',
       PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT.PRODUCT_ID AS 'PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT.PRODUCT_ID',
       PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT.MAKER_ID AS 'PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT.MAKER_ID',
       PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT.PRODUCT_NAME AS 'PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT.PRODUCT_NAME',
       PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT.SHORT_DESCRIPTION AS 'PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT.SHORT_DESCRIPTION',
       PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT.LONG_DESCRIPTION AS 'PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT.LONG_DESCRIPTION',
       PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT.CATEGORY AS 'PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT.CATEGORY',
       PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT.NEED_COLD_STORAGE AS 'PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT.NEED_COLD_STORAGE',
       PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT.BUY_PRICE AS 'PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT.BUY_PRICE',
       PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT.SELL_PRICE AS 'PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT.SELL_PRICE',
       PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT.IMAGE AS 'PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT.IMAGE',
       PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT_MAKER.MAKER_ID AS 'PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT_MAKER.MAKER_ID',
       PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT_MAKER.ADDRESS_ID AS 'PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT_MAKER.ADDRESS_ID',
       PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT_MAKER.FIRM_NAME AS 'PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT_MAKER.FIRM_NAME',
       PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT_MAKER.TELEPHONE_NO AS 'PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT_MAKER.TELEPHONE_NO',
       PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT_MAKER.E_MAIL AS 'PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT_MAKER.E_MAIL',
       PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT_MAKER_ADDRESS.ADDRESS_ID AS 'PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT_MAKER_ADDRESS.ADDRESS_ID',
       PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT_MAKER_ADDRESS.COUNTRY AS 'PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT_MAKER_ADDRESS.COUNTRY',
       PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT_MAKER_ADDRESS.TOWN AS 'PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT_MAKER_ADDRESS.TOWN',
       PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT_MAKER_ADDRESS.POSTAL_CODE AS 'PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT_MAKER_ADDRESS.POSTAL_CODE',
       PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT_MAKER_ADDRESS.BUILDING_NO AS 'PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT_MAKER_ADDRESS.BUILDING_NO',
       PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT_MAKER_ADDRESS.STREET AS 'PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT_MAKER_ADDRESS.STREET',
       PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT_MAKER_ADDRESS.APARTMENT_NO AS 'PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT_MAKER_ADDRESS.APARTMENT_NO',
       PRODUCT_IN_STORAGE_STORAGE.STORAGE_ID AS 'PRODUCT_IN_STORAGE_STORAGE.STORAGE_ID',
       PRODUCT_IN_STORAGE_STORAGE.ADDRESS_ID AS 'PRODUCT_IN_STORAGE_STORAGE.ADDRESS_ID',
       PRODUCT_IN_STORAGE_STORAGE.MANAGER_ID AS 'PRODUCT_IN_STORAGE_STORAGE.MANAGER_ID',
       PRODUCT_IN_STORAGE_STORAGE.STORAGE_NAME AS 'PRODUCT_IN_STORAGE_STORAGE.STORAGE_NAME',
       PRODUCT_IN_STORAGE_STORAGE.CAPACITY AS 'PRODUCT_IN_STORAGE_STORAGE.CAPACITY',
       PRODUCT_IN_STORAGE_STORAGE.IS_COLD_STORAGE AS 'PRODUCT_IN_STORAGE_STORAGE.IS_COLD_STORAGE',
       PRODUCT_IN_STORAGE_STORAGE_ADDRESS.ADDRESS_ID AS 'PRODUCT_IN_STORAGE_STORAGE_ADDRESS.ADDRESS_ID',
       PRODUCT_IN_STORAGE_STORAGE_ADDRESS.COUNTRY AS 'PRODUCT_IN_STORAGE_STORAGE_ADDRESS.COUNTRY',
       PRODUCT_IN_STORAGE_STORAGE_ADDRESS.TOWN AS 'PRODUCT_IN_STORAGE_STORAGE_ADDRESS.TOWN',
       PRODUCT_IN_STORAGE_STORAGE_ADDRESS.POSTAL_CODE AS 'PRODUCT_IN_STORAGE_STORAGE_ADDRESS.POSTAL_CODE',
       PRODUCT_IN_STORAGE_STORAGE_ADDRESS.BUILDING_NO AS 'PRODUCT_IN_STORAGE_STORAGE_ADDRESS.BUILDING_NO',
       PRODUCT_IN_STORAGE_STORAGE_ADDRESS.STREET AS 'PRODUCT_IN_STORAGE_STORAGE_ADDRESS.STREET',
       PRODUCT_IN_STORAGE_STORAGE_ADDRESS.APARTMENT_NO AS 'PRODUCT_IN_STORAGE_STORAGE_ADDRESS.APARTMENT_NO',
       PRODUCT_IN_STORAGE_STORAGE_EMPLOYEE.EMPLOYEE_ID AS 'PRODUCT_IN_STORAGE_STORAGE_EMPLOYEE.EMPLOYEE_ID',
       PRODUCT_IN_STORAGE_STORAGE_EMPLOYEE.USER_ID AS 'PRODUCT_IN_STORAGE_STORAGE_EMPLOYEE.USER_ID',
       PRODUCT_IN_STORAGE_STORAGE_EMPLOYEE.NAME AS 'PRODUCT_IN_STORAGE_STORAGE_EMPLOYEE.NAME',
       PRODUCT_IN_STORAGE_STORAGE_EMPLOYEE.SURNAME AS 'PRODUCT_IN_STORAGE_STORAGE_EMPLOYEE.SURNAME',
       PRODUCT_IN_STORAGE_STORAGE_EMPLOYEE.POSITION AS 'PRODUCT_IN_STORAGE_STORAGE_EMPLOYEE.POSITION',
       PRODUCT_IN_STORAGE_STORAGE_EMPLOYEE.SALARY AS 'PRODUCT_IN_STORAGE_STORAGE_EMPLOYEE.SALARY',
       PRODUCT_IN_STORAGE_STORAGE_EMPLOYEE_USER.USER_ID AS 'PRODUCT_IN_STORAGE_STORAGE_EMPLOYEE_USER.USER_ID',
       PRODUCT_IN_STORAGE_STORAGE_EMPLOYEE_USER.USERNAME AS 'PRODUCT_IN_STORAGE_STORAGE_EMPLOYEE_USER.USERNAME',
       PRODUCT_IN_STORAGE_STORAGE_EMPLOYEE_USER.PASSWORD AS 'PRODUCT_IN_STORAGE_STORAGE_EMPLOYEE_USER.PASSWORD',
       PRODUCT_IN_STORAGE_STORAGE_EMPLOYEE_USER.PERMISSION AS 'PRODUCT_IN_STORAGE_STORAGE_EMPLOYEE_USER.PERMISSION',
       PRODUCT_IN_STORAGE_STORAGE_EMPLOYEE_USER.E_MAIL AS 'PRODUCT_IN_STORAGE_STORAGE_EMPLOYEE_USER.E_MAIL'
FROM `PRODUCT_IN_STORAGE`
JOIN `PRODUCT_BATCH` `PRODUCT_IN_STORAGE_PRODUCT_BATCH` ON `PRODUCT_IN_STORAGE`.`BATCH_ID` = `PRODUCT_IN_STORAGE_PRODUCT_BATCH`.`BATCH_ID`
JOIN `PRODUCT` `PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT` ON `PRODUCT_IN_STORAGE_PRODUCT_BATCH`.`PRODUCT_ID` = `PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT`.`PRODUCT_ID`
JOIN `MAKER` `PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT_MAKER` ON `PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT`.`MAKER_ID` = `PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT_MAKER`.`MAKER_ID`
JOIN `ADDRESS` `PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT_MAKER_ADDRESS` ON `PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT_MAKER`.`ADDRESS_ID` = `PRODUCT_IN_STORAGE_PRODUCT_BATCH_PRODUCT_MAKER_ADDRESS`.`ADDRESS_ID`
JOIN `STORAGE` `PRODUCT_IN_STORAGE_STORAGE` ON `PRODUCT_IN_STORAGE`.`STORAGE_ID` = `PRODUCT_IN_STORAGE_STORAGE`.`STORAGE_ID`
JOIN `ADDRESS` `PRODUCT_IN_STORAGE_STORAGE_ADDRESS` ON `PRODUCT_IN_STORAGE_STORAGE`.`ADDRESS_ID` = `PRODUCT_IN_STORAGE_STORAGE_ADDRESS`.`ADDRESS_ID`
JOIN `EMPLOYEE` `PRODUCT_IN_STORAGE_STORAGE_EMPLOYEE` ON `PRODUCT_IN_STORAGE_STORAGE`.`MANAGER_ID` = `PRODUCT_IN_STORAGE_STORAGE_EMPLOYEE`.`EMPLOYEE_ID`
JOIN `USER` `PRODUCT_IN_STORAGE_STORAGE_EMPLOYEE_USER` ON `PRODUCT_IN_STORAGE_STORAGE_EMPLOYEE`.`USER_ID` = `PRODUCT_IN_STORAGE_STORAGE_EMPLOYEE_USER`.`USER_ID`
WHERE `PRODUCT_IN_STORAGE`.`BATCH_ID` = ?
AND `PRODUCT_IN_STORAGE`.`STORAGE_ID` = ?;